{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-theme",
  "mode": "easy",
  "compileMode": "aot",
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "app": {
    "styles": [
      "@skyux/theme/css/sky.css"
    ]
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ]
}
