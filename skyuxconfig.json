{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-theme",
  "mode": "easy",
  "compileMode": "aot",
  "host": {
    "url": "https://developer.blackbaud.com",
    "frameOptions": {
      "none": true
    }
  },
  "omnibar": {},
  "params": {
    "svcid": {
      "value": "skyux"
    }
  },
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "@blackbaud/skyux-design-tokens",
      "@skyux/icons",
      "fontfaceobserver"
    ]
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "pipelineSettings": {
    "vsts": {
      "testSettings": {
        "e2e": {
          "browserSet": false
        },
        "unit": {
          "browserSet": false
        }
      }
    }
  },
  "app": {
    "styles": [
      "@skyux/docs-tools/css/docs-tools.css"
    ],
    "theming": {
      "supportedThemes": [
        "default",
        "modern"
      ],
      "theme": "default"
    }
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ]
}
