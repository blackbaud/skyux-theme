{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-theme",
  "mode": "easy",
  "compileMode": "aot",
  "moduleAliases": {
    "@skyux/theme/css/sky.css": "src/app/public/styles/sky.scss",
    "@skyux/theme/utils/node-js/style-loader": "utils/node-js/style-loader.js",
    "@skyux/theme": "src/app/public/index.ts"
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "app": {
    "styles": [
      "src/app/public/styles/sky.scss"
    ]
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ]
}
