{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-theme",
  "mode": "easy",
  "compileMode": "aot",
  "host": {
    "url": "https://developer.blackbaud.com"
  },
  "omnibar": {
    "experimental": true
  },
  "params": {
    "svcid": {
      "value": "skyux"
    }
  },
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "@blackbaud/skyux-design-tokens",
      "@types/fontfaceobserver",
      "fontfaceobserver",
      "normalize-scss"
    ]
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "app": {
    "styles": [
      "@skyux/theme/css/sky.css",
      "@skyux/docs-tools/css/docs-tools.css"
    ]
  },
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ]
}
