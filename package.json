{
  "name": "theme-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && ng serve",
    "build": "ng build",
    "postbuild": "node ./scripts/prepare-package.js && npm run skyux:generate-documentation",
    "watch": "ng build --watch --configuration development",
    "test": "npm run build && ng test",
    "lint": "ng lint",
    "e2e": "npm run build && ng e2e",
    "skyux:generate-documentation": "ng generate @skyux-sdk/documentation-schematics:documentation",
    "skyux:generate-lib-resources-module": "ng generate @skyux/i18n:lib-resources-module",
    "skyux:format": "npx prettier --write ."
  },
  "private": true,
